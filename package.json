{
  "name": "@gerhobbelt/gulp-babel",
  "version": "8.0.0-5",
  "description": "Use next generation JavaScript, today",
  "license": "MIT",
  "repository": "babel/gulp-babel",
  "author": "Sindre Sorhus <sindresorhus@gmail.com> (sindresorhus.com)",
  "contributors": [
    "Charles Samborski <demurgos@demurgos.net> (demurgos.net)"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "npm run lint && npm test",
    "test": "xo && mocha",
    "test-only": "mocha",
    "lint": "xo",
    "fix": "xo --fix",
    "prettier": "prettier --write *.js && xo --fix",
    "pub": "npm publish --access public"
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "babel",
    "transpiler",
    "es2015",
    "es2016",
    "es2017",
    "rewriting",
    "transformation",
    "syntax",
    "codegen",
    "desugaring",
    "javascript",
    "compiler"
  ],
  "dependencies": {
    "plugin-error": "1.0.1",
    "replace-ext": "1.0.0",
    "through2": "3.0.1",
    "vinyl-sourcemaps-apply": "0.2.1"
  },
  "peerDependencies": {
    "@gerhobbelt/babel-core": "~7.2.1-26"
  },
  "devDependencies": {
    "@gerhobbelt/babel-core": "7.2.1-26",
    "@gerhobbelt/babel-plugin-transform-arrow-functions": "7.2.1-26",
    "@gerhobbelt/babel-plugin-transform-block-scoping": "7.2.1-26",
    "@gerhobbelt/babel-plugin-transform-classes": "7.2.1-26",
    "gulp-sourcemaps": "2.6.5",
    "mocha": "6.1.4",
    "pre-commit": "1.2.2",
    "prettier": "1.17.1",
    "vinyl": "2.2.0",
    "xo": "0.24.0"
  },
  "xo": {
    "envs": [
      "node",
      "mocha"
    ]
  }
}
